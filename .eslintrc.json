{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "airbnb",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": "**/tsconfig.json",
        "tsconfigRootDir": " __dirname"
    },
    "rules": {
        "import/prefer-default-export": "off"
    }
}